<p id="notice"><%= notice %></p>

<h1>Responses</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Score</th>
      <th>Time entered</th>
      <th>Question number</th>
      <th>Submission</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @responses.each do |response| %>
      <tr>
        <td><%= response.question %></td>
        <td><%= response.score %></td>
        <td><%= response.time_entered %></td>
        <td><%= response.question_number %></td>
        <td><%= response.submission_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
